{
  "name": "angular-auto",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm install && ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "server": "node local.js",
    "build:prod": "npm run build:browser:prod && npm run build:server:prod",
    "start:prerender": "npm run build:prerender & npm run serve:prerender",
    "serve:prerender": "node static.js",
    "build:prerender": "npm run build:prod && node dist/prerender.js",
    "build:browser:prod": "ng build --prod",
    "build:browser:serverless": "ng build --prod --base-href /production/",
    "build:serverless": "npm run build:browser:serverless && npm run build:server:serverless",
    "build:prod:deploy": "npm run build:prod && npm run deploy",
    "build:serverless:deploy": "npm run build:serverless && npm run deploy",
    "deploy": "serverless deploy",
    "build:server:prod": "ng run angular-auto:server && webpack --config webpack.server.config.js --progress --colors",
    "build:server:serverless": "ng run angular-auto:server:serverless && webpack --config webpack.server.config.js --progress --colors",
    "compile:server_bak": "webpack --config webpack.server.config.js --progress --colors",
    "build:ssr_bak": "npm run build:client-and-server-bundles && npm run compile:server",
    "serve:ssr_bak": "node local.js",
    "build:client-and-server-bundles_bak": "ng build --prod && ng run angular-auto:server:production --bundleDependencies all",
    "dev:ssr": "ng run angular-auto:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run angular-auto:server:production",
    "prerender": "ng run angular-auto:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "^9.1.13",
    "@angular-devkit/schematics": "^9.1.13",
    "@angular/animations": "^9.1.13",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.13",
    "@angular/compiler": "^9.1.13",
    "@angular/core": "^9.1.13",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^9.1.13",
    "@angular/localize": "^9.1.13",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.1.13",
    "@angular/platform-browser-dynamic": "^9.1.13",
    "@angular/platform-server": "^9.1.13",
    "@angular/router": "^9.1.13",
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@material/animation": "^3.1.0",
    "@ng-bootstrap/ng-bootstrap": "^5.3.0",
    "@ng-toolkit/serverless": "^8.1.0",
    "@ng-toolkit/universal": "^8.1.0",
    "@nguniversal/common": "9.1.1",
    "@nguniversal/express-engine": "9.1.1",
    "@ngxs/store": "^3.6.2",
    "@schematics/angular": "^9.1.13",
    "aws-serverless-express": "^3.3.6",
    "bootstrap": "^4.4.1",
    "cors": "^2.8.5",
    "domino": "^2.1.4",
    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "jspdf": "^1.5.3",
    "ng2-currency-mask": "^5.3.1",
    "ngx-spinner": "^8.1.0",
    "rxjs": "~6.6.3",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.13",
    "@angular/cli": "^9.1.13",
    "@angular/compiler-cli": "^9.1.13",
    "@angular/language-service": "^9.1.13",
    "@nguniversal/builders": "^9.1.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^8.9.5",
    "codelyzer": "^5.2.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "opencollective": "^1.0.3",
    "protractor": "^5.4.3",
    "serverless": "^1.60.0",
    "serverless-api-compression": "^1.0.1",
    "serverless-apigw-binary": "^0.4.4",
    "ts-loader": "^6.2.1",
    "ts-node": "~7.0.0",
