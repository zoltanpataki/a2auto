{
  "name": "angular-auto",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm install && ng serve",
    "build": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "server": "node local.js",
    "build:prod": "npm run build:browser:prod && npm run build:server:prod",
    "start:prerender": "npm run build:prerender & npm run serve:prerender",
    "serve:prerender": "node static.js",
    "build:prerender": "npm run build:prod && node dist/prerender.js",
    "build:browser:prod": "ng build --configuration production",
    "build:prod:deploy": "npm run build:prod && npm run deploy",
    "build:server:prod": "ng run angular-auto:server && webpack --config webpack.server.config.js --progress --colors",
    "compile:server_bak": "webpack --config webpack.server.config.js --progress --colors",
    "build:ssr_bak": "npm run build:client-and-server-bundles && npm run compile:server",
    "serve:ssr_bak": "node local.js",
    "build:client-and-server-bundles_bak": "ng build --configuration production && ng run angular-auto:server:production --bundleDependencies all",
    "dev:ssr": "ng run angular-auto:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --configuration production && ng run angular-auto:server:production",
    "prerender": "ng run angular-auto:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/architect": "^0.1202.18",
    "@angular-devkit/core": "^12.2.18",
